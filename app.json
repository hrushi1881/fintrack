{
  "expo": {
    "name": "FinTrack",
    "slug": "fintrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fintrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Comprehensive personal finance management app to track expenses, manage budgets, set financial goals, handle liabilities, and gain insights into your spending habits.",
    "privacy": "public",
    "primaryColor": "#4F6F3E",
    "privacyPolicy": "https://your-privacy-policy-url.com/privacy",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fintrack.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": []
    },
    "android": {
      "package": "com.fintrack.app",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "permissions": [],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#99D795",
          "dark": {
            "backgroundColor": "#99D795"
          }
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "eas": {
        "projectId": "b543f9a1-7def-4655-9882-ad07d8955a06"
      }
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
